<!--index.wxml-->
<view class="container">
  <view class="banner-container">
    <yu-swiper indicator-dots="{{true}}" autoplay="{{true}}">
      <yu-swiper-slide>
        <view class="test-img yellow"></view>
      </yu-swiper-slide>
      <yu-swiper-slide>
        <view class="test-img gray"></view>
      </yu-swiper-slide>
      <yu-swiper-slide>
        <view class="test-img green"></view>
      </yu-swiper-slide>
      <yu-swiper-slide>
        <view class="test-img" style="background: #EB9352;"></view>
      </yu-swiper-slide>
      <yu-swiper-slide>
        <view class="test-img" style="background: #7CC0A2;"></view>
      </yu-swiper-slide>
    </yu-swiper>
  </view>
  <view class="product-container">
    <view class="product__item">
      <yu-swiper indicator-dots="{{false}}" >
        <yu-swiper-slide>
          <view class="test-img yellow"></view>
        </yu-swiper-slide>
        <yu-swiper-slide>
          <view class="test-img gray"></view>
        </yu-swiper-slide>
        <yu-swiper-slide>
          <view class="test-img green"></view>
        </yu-swiper-slide>
        <yu-swiper-slide>
          <view class="test-img" style="background: #EB9352;"></view>
        </yu-swiper-slide>
        <yu-swiper-slide>
          <view class="test-img" style="background: #7CC0A2;"></view>
        </yu-swiper-slide>
      </yu-swiper>
    </view>
  </view>
  <view class="footer">
    
  </view>
</view>
